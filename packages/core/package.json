{
  "name": "@nteract/core",
  "version": "1.0.6",
  "description": "core modules and components for nteract apps",
  "main": "lib/index.js",
  "nteractDesktop": "src/index.js",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm run build:flow",
    "build": "npm run build:clean && npm run build:lib",
    "build:clean": "rimraf lib",
    "build:flow": "flow-copy-source -v -i '**/__tests__/**' src lib",
    "build:lib": "babel -d lib src --ignore '**/__tests__/**'",
    "build:lib:watch": "npm run build:lib -- --watch",
    "build:watch": "npm run build:clean && npm run build:lib:watch && npm run build:flow"
  },
  "dependencies": {
    "@nteract/commutable": "^3.0.4",
    "@nteract/display-area": "^3.1.7",
    "@nteract/dropdown-menu": "^0.0.5",
    "@nteract/editor": "^5.0.6",
    "@nteract/octicons": "^0.0.5",
    "@nteract/transforms": "^3.1.4",
    "@nteract/types": "^1.0.4",
    "commonmark": "^0.28.0",
    "commonmark-react-renderer": "^4.3.3",
    "date-fns": "^1.29.0",
    "escape-carriage": "^1.2.0",
    "mathjax-electron": "^2.0.1",
    "prop-types": "^15.5.10",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-redux": "^5.0.6",
    "react-syntax-highlighter": "^6.0.0",
    "redux": "^3.7.2",
    "rxjs": "^5.5.6",
    "uuid": "^3.1.0"
  },
  "peerDependencies": {
    "immutable": "^4.0.0-rc.9",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-jsx": "^2.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nteract",
    "ohgodamonomoduleinamonorepo"
  ],
  "author": "Kyle Kelley <rgbkrk@gmail.com>",
  "license": "BSD-3-Clause"
}
