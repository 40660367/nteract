{
  "defaultSeverity": "warn",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-react-a11y",
    "tslint-react-perf",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "no-unnecessary-type-assertion": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "ordered-imports": false,
    "no-duplicate-imports": true,
    "max-line-length": false,
    "trailing-comma": false,
    "member-access": false
  },
  "rulesDirectory": []
}
